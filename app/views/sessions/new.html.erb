
<h1>Please log in</h1>


<%= form_tag sessions_path do %>
    <%= label_tag "Username" %>
    <br>
    <%= text_field_tag :username, nil, placeholder: "Username" %>
    <br>
    <%= label_tag "Password" %>
    <br>
    <%= password_field_tag :password, nil, placeholder: "Password" %>
    <br>
    <br>
    <%= submit_tag "Log In"%>
<% end %>

<nav>
<div class = "footer">
  <%= link_to 'About Page', about_path %>
</div>
</nav>